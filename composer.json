{
    "name": "ch1p/picofeed8",
    "description": "RSS/Atom parsing library",
    "homepage": "https://github.com/ch1p/picofeed8",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Frédéric Guillot"
        },
        {
            "name": "Nicolas Bailly"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-iconv": "*",
        "ext-dom": "*",
        "ext-xml": "*",
        "ext-libxml": "*",
        "ext-SimpleXML": "*",
        "laminas/laminas-xml": "^1.0",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "kevinrob/guzzle-cache-middleware": "^2.1"
    },
    "require-dev" : {
        "phpdocumentor/reflection-docblock": "2.0.4",
        "monolog/monolog": "^1.23",
        "blastcloud/guzzler": "^2.1"
    },
    "suggest": {
        "ext-curl": "PicoFeed will use cURL if present",
        "monolog/monolog": "You can set a monolog Logger to get debug information from PicoFeed"
    },
    "autoload": {
        "psr-0": {"PicoFeed": "lib/"}
    },
    "bin" : [
        "picofeed"
    ]
}
